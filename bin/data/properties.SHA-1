number-of-initial-hash-values=5
number-of-round-constants=4
number-of-rounds=80
input-bytes-in-each-block=55				// 55 bytes (input) + 1 byte (for '1' bit) + 8 bytes (for appending length)...
buffer-matrix-rows=2
block-size=64								// 512 bits = (512 / 8) -> 64 bytes = (64 / 4) -> 16 integers...